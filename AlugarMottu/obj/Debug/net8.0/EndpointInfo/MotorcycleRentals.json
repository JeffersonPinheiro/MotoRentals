  "components": {
    "schemas": {
      "CNHType": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      },
      "DeliveryManCreateDto": {
        "required": [
          "birthDate",
          "cnhNumber",
          "cnhType",
          "cnpj",
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "cnpj": {
            "minLength": 1,
            "pattern": "^\\d{14}$",
            "type": "string"
          },
          "birthDate": {
            "type": "string",
            "format": "date"
          },
          "cnhNumber": {
            "minLength": 1,
            "pattern": "^\\d{11}$",
            "type": "string"
          },
          "cnhType": {
            "$ref": "#/components/schemas/CNHType"
          }
        },
        "additionalProperties": false
      },
      "MotorcycleCreateDto": {
        "required": [
          "model",
          "plate",
          "year"
        ],
        "type": "object",
        "properties": {
          "year": {
            "maximum": 2100,
            "minimum": 1900,
            "type": "integer",
            "format": "int32"
          },
          "model": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "plate": {
            "minLength": 1,
            "pattern": "^[A-Z]{3}-\\d{4}$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "MotorcycleUpdatePlateDto": {
        "type": "object",
        "properties": {
          "plate": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RentalCreateDto": {
        "type": "object",
        "properties": {
          "deliveryManId": {
            "type": "string",
            "format": "uuid"
          },
          "motorcycleId": {
            "type": "string",
            "format": "uuid"
          },
          "planType": {
            "$ref": "#/components/schemas/RentalPlanType"
          },
          "startDate": {
            "type": "string",
            "format": "date-time"
          },
          "predictedEndDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "RentalPlanType": {
        "enum": [
          7,
          15,
          30,
          45,
          50
        ],
        "type": "integer",
        "format": "int32"
      },
      "RentalReturnDto": {
        "type": "object",
        "properties": {
          "rentalId": {
            "type": "string",
            "format": "uuid"
          },
          "returnDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      }
    }
  }
